  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "@translations/i18next": "<rootDir>/src/lib/i18next",
      "@tests/renders": "<rootDir>/src/setupTestRenders",
      "@tests/mock/(.*)/(.*)": "<rootDir>/src/mocks/$1/$2"
    }
  }
}
